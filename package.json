{
  "name": "betmatch",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./src/server.ts",
    "start": "npm run prod:migrate && node dist/server.js",
    "prod:migrate": "prisma migrate deploy",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "prebuild": "rm -rf dist",
    "test": "cross-env NODE_ENV=test dotenv_config_path=./.env.test jest --passWithNoTests --runInBand",
    "test:migration:run": "cross-env NODE_ENV=test dotenv_config_path=./.env.test jest --passWithNoTests --runInBand",
    "test:migration:generate": "npm run test:load-envs prisma migrate dev",
    "test:watch": "jest --watch --passWithNoTests --runInBand",
    "test:load-envs": "node load-envs.js",
    "test:seed": "npm run test:load-envs prisma db seed",
    "test:coverage": "cross-env NODE_ENV=test dotenv_config_path=./.env.test jest --passWithNoTests --runInBand --coverage --collectCoverageFrom='./src/**'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "http-status": "^1.7.0",
    "joi": "^17.10.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@types/cors": "^2.8.14",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/supertest": "^2.0.13",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.3.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "ttypescript": "^1.5.15",
    "typescript": "^5.2.2",
    "typescript-transform-paths": "^3.4.6"
  }
}
